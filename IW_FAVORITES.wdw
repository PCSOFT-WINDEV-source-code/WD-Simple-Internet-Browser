     -
       name : OPT_IMPORT_FAVORITES
       identifier : 0x203fd600146fd929
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes :
         -
           internal_properties : CAAAAAgAAACxKHcdzAW2fL1J6TsagXoaS4NWFT/heaopfqCJiRcY7AEbY2i8sI4klNoUEHPtNckrbBzdjM94WzPe3jJ55mz+6kxiXocu42ZKs/teCAiTmVcEkR9yxvu6bhsFXzbi4bERE7JtHHx3PP5gEF7wdf+5jjyaNkkIjen0wReeukFuYPtb6x4vQ+TZMixXAZzzXp0=
           code : |1-
            
            sFile is string = fSelect("", "", "Open", "HTML documents (*.HTML)" + TAB + "*.HTM;*.HTML", "*.HTML", fselOpen)
            if sFile <> "" then
            	
            	// Load Netscape-Bookmark-File documents
            	
            	// Opens the document 
            	htmlFavorites is htmlDocument = HTMLOpen(sFile, fromFile)
            	
            	HTMLIterateChildren( htmlFavorites.html.body )
            	INTERNAL PROCEDURE HTMLIterateChildren( htmlRoot is htmlNode )
            		
            		// Lists child nodes
            		for each htmlNode of htmlRoot..ChildNode
            			
            			// Only take into account <A> tags in the document 
            			if htmlNode..TagName ~= "A" then
            				
            				stEntry is FavoriteEntry
            				
            				when exception in
            					
            					stEntry.Date		= EpochToDateTime(htmlNode..Attribute[ "add_date" ]..Value)
            					stEntry.Name		= htmlNode..Text
            					stEntry.URL			= htmlNode..Attribute[ "href" ]..Value
            					stEntry.icon		= htmlNode..Attribute[ "icon" ]..Value
            					
            				do
            					
            					VariableReset(stEntry)
            					
            				end
            				
            				// Check if there is an identical entry already
            				if stEntry.URL <> "" _and_ FavoriteExists( stEntry.URL ) = False then
            					
            					// Adds the entry
            					Favorites.Add(stEntry)	
            				end
            			else
            				HTMLIterateChildren( htmlNode )
            			end			
            		end
            	end
            	
            	// Save
            	SaveFavorites()
            	
            	// Resets the list and the view
            	UpdateFavorites()
            end
           type : 32
     -
       name : OPT_EXPORT_FAVORITES
       identifier : 0x203fd6001470d929
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes :
         -
           internal_properties : CAAAAAgAAABDndC32EV1pph6asB2Ff9PVjQD+Tonq/dxrMDV4gfP74ALYP6VBvrFD/+DOe6w8YvbHsp3JOuxxJK90bbP+q80oCIHCGA0QF+E2uVn+Oqa1Hln6TsUv773IBRv8qiasJC1CAB7zOcmDVam0loqEb7cvKe8COGDYDis+BOXifJ9D69WQ2KxtJWUVuP6/Zc+pA==
           code : |1+
            
            // Saves the favorites in NETSCAPE-Bookmark-file format
            
            Epoch			is unsigned int	= DateTimeToEpoch(Now, epochUnixSecond)
            sGeneratedName	is string		= "favorites_" + DateToString(Today, "MM_DD_YYYY") + ".html"
            sFile			is string
            
            // Opens the file picker
            sFile = fSelect("", sGeneratedName, "Save as...", "All files (*.*)" + TAB + "*.*" + CR + "HTML documents (*.HTML)" + TAB + "*.HTM;*.HTML", "*.HTM;*.HTML", fselCreate + fselExist)
            if sFile <> "" then
            	
            	// File header
            	sHTML is string = [
            	<!DOCTYPE NETSCAPE-Bookmark-file-1>
            	<!-- This is an automatically generated file.
            		It will be read and overwritten.
            		DO NOT EDIT! -->
            	<META HTTP-EQUIV="Content-Type" CONTENT="text/html; charset=UTF-8">
            	<TITLE>Bookmarks</TITLE>
            	<H1>Bookmarks</H1>
            	<DL><p>
            		<DT><H3 ADD_DATE="[%Epoch%]" LAST_MODIFIED="[%Epoch%]" PERSONAL_TOOLBAR_FOLDER="true">Favorites bar</H3>
            		<DL><p>
            	
            	]
            	
            	// Generates the entries 
            	stFavorite is FavoriteEntry
            	for each stFavorite of Favorites
            		
            		sHTML += [
            			<DT><A HREF="[%stFavorite.URL%]" ADD_DATE="[%stFavorite.Date.ToEpoch( epochUnixSecond )%]" ICON="[%stFavorite.Icon%]">[%stFavorite.Name%]</A>
            		
            		]
            		
            	end
            	
            	sHTML += [
            	</DL><p>
            	]
            	
            	// Save
            	fSaveText(sFile, sHTML)
            end
           type : 32
