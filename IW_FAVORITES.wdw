  procedures :
   -
     name : UpdateFavorites
     procedure_id : 1170889723653445913
     type_code : 14
     code : |1-
      // Summary : Updates the favorites
      // Syntax:
      // UpdatesFavorites ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      procedure UpdateFavorites()
      
      MyWindow.DisplayEnabled = False
      
      // Clears the list
      LOOP_FAVORITE.DeleteAll()
      
      // For each favorite
      for each stFavorite of Favorites
      	
      	// Adds a row
      	let N = LOOP_FAVORITE.AddLine()
      	
      	// Fills the information
      	LOOP_FAVORITE[ N ].STC_TITLE_FAVORITE	= stFavorite.Name
      	LOOP_FAVORITE[ N ].STC_URL				= stFavorite.URL
      	LOOP_FAVORITE[ N ].IMG_FAVICON			= DataURIToImage( stFavorite.icon )
      	
      end
      
      MyWindow.DisplayEnabled			= True
      
      STC_TC_EMPTY.Visible			= ( Favorites.Empty )
      OPT_DELETE_ALL.Grayed			= ( Favorites.Empty )
      OPT_EXPORT_FAVORITES.Grayed		= ( Favorites.Empty )
     type : 458752
