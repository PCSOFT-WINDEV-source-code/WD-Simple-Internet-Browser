           code : |1-
            
            let REGEXPURL = "((^(http)|^(HTTP))[sS]?(://)[a-zA-Z0-9\./%_\-]+[\?]?[a-zA-Z0-9\./\=&\+%_\-]*)|(^[a-zA-Z0-9\./%_\-]+\.[a-zA-Z0-9\./%_\-]*[a-zA-Z0-9\./%_\-]+[\?]?[a-zA-Z0-9\./\=&\+%_\-]*)"
            
