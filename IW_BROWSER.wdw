      -
        internal_properties : 
        code : |1-
         procedure MyWindow( gsURL is string = "", gsAlias is string = "" )
         
         SourceUrl is énumération
         	SourceClick					// A click triggers the URL change
         	SourceHistory			// Change URL from the history
         	SourceInput				// Change URL from an entry in the address bar
         	SourceOpen			// Apply the first URL when the tab is opened 
         end
         
         // System URLs
         constant
         	Prefix_System_URL		= "://"
         	URL_Favorites			= Prefix_System_URL + "bookmarks"<§aad:0§>
         	URL_Settings			= Prefix_System_URL + "settings"
         	URL_History				= Prefix_System_URL + "history"
         	URL_Home				= Prefix_System_URL + "home"
         end
         
         __icon				is Image						// Icon of the displayed site ("favicon")
         __WebAlias			is string						// Browser tab alias
         __URL				is string						// Current URL
         __Title				is string	= "New tab"			// Browser tab title
         __Pane				is Control						// Tab associated with the internal window
         __LocalHistory		is array of HistoryEntry		// Browsing history local to the internal window
         __HistoryIndex		is int							// Index in local browsing history
         __Source			is SourceUrl					// URL change source
         
         // Initial display
         BTN_REFRESH.Plane		= 0
         BTN_BRW_SEARCH.Grayed	= True
         BTN_REFRESH.Grayed		= True
         BTN_PREVIOUS.Grayed		= True
         BTN_NEXT.Grayed			= True
      -
        code : |1-
         
         // Stores the associated tab
         __Pane <- ControlPane(MyInternalWindow)
         
         // Changes the URL change source
         __Source				= SourceOpen
         
         // Browses to the URL
         IW_BROWSER.WebAlias		= gsAlias
         IW_BROWSER.URL			= gsURL
         
         // If a blank tab was opened
         if gsURL = "" then
         	
         	ProcessSystemURL( URL_Home )
         	
         	// Enables the edit control and activates input 
         	EDT_BRW.State = Active
         	ForceInput()
         	
         	INTERNAL PROCEDURE ForceInput() < delay = 5ms >
         		
         		// No URL specified
         		BTN_BRW_SEARCH.Plane	= 0
         		IMG_LOADING.Plane		= 1
         		SetFocus(EDT_BRW)
         	end
         end
        type : 14
      -
        code : |1-
         
         // Save history
         SaveHistory()
