     name : TriggerSearch
     procedure_id : 1170889736540186559
     type_code : 14
     code : |1-
      // Summary: Starts a search
      // Syntax:
      //TriggerSearch (<sSearch> is string)
      //
      // Parameters:
      //	sSearch (ANSI string): Search keywords
      // Return value:
      // 	None
      //
      procedure TriggerSearch( sSearch is string )
      
      // Searches for the default engine
      let N = DefaultSearchEngines.Seek(asLinear, "ByDefault", True)
      
      // No default engine, use the first one in the list
      if N < 1 then N = 1
      
      // Builds the search URL ( UTF )
      sURL is string = DefaultSearchEngines[ N ].URL.Remplace( "%s", sSearch.VersUTF8().URLEncode(encodeURLFromUTF8) )
      
      // Changes URL
      IW_BROWSER.URL = sURL
     type : 458752
   -
     name : NewHistoryEntry
     internal_properties : CAAAAAgAAAB1AXFvQ4H6X5WOe3BysglbGAKtepdFCIe1H53fHYHlsGJvKZZLqsFE1PDwWdempa4D7xXrHocMl8Jv0+h5dQuGLBAG7APwLlQUzbueTtJt1DLn8hZYKNnq+FXB6oF3UjDKVHUiWRM4xyUHZ2z5BR9H4gjkPLfm909m083Qbdb6eQ==
     procedure_id : 1170889736540252095
     type_code : 14
     code : |1-
      // Summary: Stores a new history entry
      // Syntax:
      //NewHistoryEntry (<stEntry> is HistoryEntry)
      //
      // Parameters:
      //	stEntry (HistoryEntry): History entry to store
      // Return value:
      // 	None
      //
      procedure NewHistoryEntry( stEntry is HistoryEntry )
      
      // Ignores entry if comes from the history
      if __Source = ( SourceHistory ) _or_ stEntry.URL = "" then
      	return
      end
      
      // Checks if it is a system URL
      if stEntry.URL not [= "://" then
      	
      	// Stores entry in the general history
      	History.Add(stEntry)		
      	
      	// Updates the history tab
      	if ControlExist("IW_HISTORY") then
      		IW_HISTORY.RefreshHistory()
      	end
      	
      	// Update
      	HistoryWithoutDuplicate <= History
      	HistoryWithoutDuplicate.DeleteDuplicate(asMember, "URL")
      end
      
      // If the Back button has not been used
      if __HistoryIndex = __LocalHistory.Count then
      	
      	// Also stores in the local history
      	__HistoryIndex = __LocalHistory.Add(stEntry)
      	
      	if __Source <> SourceOpen then			
      		BTN_PREVIOUS.Grayed	= False
      		BTN_NEXT.Grayed		= True
      	end
      	
      else
      	
      	// Truncates local history at current index
      	Dimension(__LocalHistory, __HistoryIndex)		
      	
      	// Also stores in the local history
      	__HistoryIndex = __LocalHistory.Add(stEntry)	
      	
      	if __Source <> SourceOpen then
      		BTN_PREVIOUS.Grayed	= False
      		BTN_NEXT.Grayed		= True
      	end
      end
     type : 458752
   -
     name : ProcessHistoryFavorite
     internal_properties : CAAAAAgAAAA70H5bhIXKI8wAQbMsHj3hvu98z4qau7+P4bvBZ0cFwvLXzSLNZIsEYsqeS9FEgQTrn+nn3GUab+AZPUaPt/fkfdFF7RyLxctB+kYrpUPqYZvYiY/Bm0wFwZaA26hoZ8Vj/aR7op5T5l5BejaWhZAqKVd7vX8CA3z7zGzYAbe0tw==
     procedure_id : 1170889736540317631
     type_code : 14
     code : |1-
      // Summary: <specify the action of the procedure>
      // Syntax:
      // ProcessHistoryFavorite ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      procedure ProcessHistoryFavorite() < delay = 5ms >
      
      // Reads the URL again
      sUrl	is string	= IW_BROWSER.URL
      
      // Creates an entry to store in the history
      stEntry	is HistoryEntry
      stEntry.Date	= SysDateTime
      stEntry.icon	= ImageToDataURI( __icon )
      stEntry.Title	= IW_BROWSER.PageTitle
      stEntry.URL		= sUrl 
      
      // Stores the entry
      NewHistoryEntry( stEntry )
      
      // Tests the URL added as favorite
      UrlTestFavorite( sUrl )
      
      BTN_FAVORITES_ADD.Grayed	= False
      OPT_FAVORITE_ADD.Grayed		= False
      
      // Restores the source of the URL change
      __Source					= SourceClick
     type : 458752
   -
     name : UrlTestFavorite
     internal_properties : CAAAAAgAAAAUD9XQvwTSt/rQJlRfNfBtCS8xeXS6ITpSY+V+rTpsDYpKaq2ql3vtZTwxuoWwFogXcOBq64YysUbf8/GgxrDLzkwkxRXtaxui6tFTSBm+tQvXPS58iDZRHZxLV2p8LSq65nYG3tdn/fflSKRIj47pFrnamwLunTQ/Azou5KQLgtBJHo7DrRvYPsuJJGvzbcFQ
     procedure_id : 1170889736540383167
     type_code : 14
     code : |1-
      // Summary: Processes a URL for favorites
      // Syntax:
      //UrlTestFavorite (<sUrl> is string)
      //
      // Parameters:
      //	sUrl (ANSI string): Url to be tested
      // Return value:
      // 	None
      //
      procedure UrlTestFavorite( sUrl is string )
      
      // If the page has already been added to favorites
      if FavoriteExists( sUrl ) then
      	
      	BTN_FAVORITES_ADD.Image		= IMG_IS_FAVORITE
      	BTN_FAVORITES_ADD.ToolTip	= "Edit favorite"
      	BTN_FAVORITES_ADD.Note		= favoriteAdded
      	
      	// The page has not been added to favorites yet
      else
      	BTN_FAVORITES_ADD.Image		= IMG_ADD_FAVORITE
      	BTN_FAVORITES_ADD.ToolTip	= "Add this page to favorites (Ctrl + D)"
      	BTN_FAVORITES_ADD.Note		= favoriteUnknown
      end
     type : 458752
   -
     name : ProcessSystemURL
     internal_properties : CAAAAAgAAACgVGX1Y7Jqt+XO3JaHT4aZMc+vCWJ4HcxQKwC4YmBG3eIOaupTphtf5dzpsoWYBgCX+KhCa27S6ZYXc4EYxqhbhlwEJX09Y1sGRmVHRJUCYWdr2ZLQPJqNYUjvy5bYIVZ2GqJqYks7KZN3+uOULLHGCwCx1vxY/r8OQOffB3sKl/UwBammIvITRuO5Fl3mPnXk
     procedure_id : 1170889736540448703
     type_code : 14
     code : |1-
      // Summary: Processes a system URL
      // Syntax:
      //ProcessSystemURL (<sUrl> is string)
      //
      // Parameters:
      //	sUrl (ANSI string): URL to be processed
      // Return value:
      // 	None
      //
      procedure ProcessSystemURL( sUrl is string )
      
      // If it is a system URL
      // Check if there is an open tab with this URL
      if sUrl [= Prefix_System_URL then
      	
      	// Gets the Tab control from the tab
      	tTab is Control <- { ControlTab(MyInternalWindow), indControl }
      	
      	// Lists the tabs
      	let N = 1 
      	let Alias = EnumControl(tTab, N)
      	
      	while Alias <> ""
      		
      		// References the browser open in the tab
      		oBrowser is Control <- tTab[ Alias ].IW_BROWSER
      		
      		// Checks the URL
      		if oBrowser.__URL = sUrl then
      			
      			// Forces the tab to be displayed
      			tTab			= Alias
      			return			
      		end			
      		
      		// Next tab
      		N ++
      		Alias = EnumControl(tTab, N)
      	end
      end
      
      // Synchronizes the edit control
      __URL	= sUrl
      EDT_BRW	= sUrl
      EDT_BRW.ExecuteProcess(trtExit)
      
      if sUrl ~= URL_Settings
      	
      	// Opens the parameters
      	ShowSettings()
      	
      else if sUrl ~= URL_Favorites
      	
      	// Opens the favorites
      	ShowFavorites()
      	
      else if sUrl ~= URL_History
      	
      	// Opens the history
      	ShowHistory()
      	
      else if sUrl ~= URL_Home
      	
      	// Opens the home page
      	ShowHome()
      	
      end
     type : 458752
   -
     name : OpenBrowser
     internal_properties : CAAAAAgAAAA3EaWf1g1/MgCrTCSPLZ/icw54fZ1TL0TtYBzAwlPS8DzSKIOEseMJdr1zrz8RhKuUt9ndN+3l2TmoYU8X+ccOjI2TZ8sQHp3+v43CcKoEtnt1D88i/C0tiEaUZavawEFlPlT8oZYMUUpMaNnceqVFO/3UYQ+2p7XbecNBX5Gj3uHQxS8M6eXeWgeTloS9wQvSl23n967kbc9f85I0NNlZGFp5s7pQfcyk1eAZkMM/
     procedure_id : 1170889736540514239
     type_code : 14
     code : |1-
      // Summary: Opens a URL with a new tab
      // Syntax:
      //OpenBrowser (<sUrl> is string [, <sAlias> is string [, <bKeepCurrentTab> is boolean [, <bDetachTab> is boolean]]])
      //
      // Parameters:
      //	sUrl (ANSI string): URL to open
      //	sAlias (ANSI string - default value=""): Alias of this browser
      //	bKeepCurrentTab (boolean - default value=1): True if the current tab must remain in the foreground, False otherwise
      //	bDetachTab (boolean - default value=0): True to detach the new tab, False otherwise
      // Return value:
      // 	None
      //
      procedure OpenBrowser( sUrl is string, sAlias is string = "", local bKeepCurrentTab is boolean = True, bDetachTab is boolean = False ) 
      
      sAliasOpen is string
      
      if bDetachTab then
      	
      	// Opens a new window
      	WIN_MAIN.OpenSister(sUrl)
      	bKeepCurrentTab = False
      	return
      end
      
      MyWindow.DisplayEnabled = False
      
      // Gets the Tab control from the tab
      tTab is Control <- { ControlTab(MyInternalWindow), indControl }
      
      // If an alias was specified
      if sAlias <> "" then
      	
      	// Check if there is an open tab with that alias
      	
      	// Lists the tabs
      	let N = 1 
      	let Alias = EnumControl(tTab, N)
      	
      	while Alias <> ""
      		
      		// If the alias matches
      		if Alias ~= sAlias then
      			
      			// References the browser open in the tab
      			oBrowser is Control <- tTab[ Alias ].IW_BROWSER
      			
      			// Requests the tab to show the URL
      			oBrowser.URL = sUrl
      			
      			if bKeepCurrentTab = False then
      				
      				// Forces the tab to be displayed
      				tTab		= Alias
      				sAliasOpen	= Alias
      			end
      			
      			return
      		end
      		
      		// Next tab
      		N ++
      		Alias = EnumControl(tTab, N)
      	end
      end
      
      // Opens a new tab
      sAliasOpen = tTab.Ouvre( ( sUrl = "" ? "New tab" else "Load..." ), IW_BROWSER, sUrl, sAlias )
      
      end:
      
      if bKeepCurrentTab then
      	
      	// Restores the displayed tab
      	tTab		= __Pane.alias
      	sAliasOpen	= __Pane.alias		
      	
      end
      
      
      
      MyWindow.DisplayEnabled = True
     type : 458752
   -
     name : ChangeTabIcon
     internal_properties : CAAAAAgAAAC1nHREUbd0F0MD/FmSonInUEtZXqO2s5hbGaOhZy9FCgqH/Rp9xFMUumr+Y2kk+dTbp8G35H0aZ9Ch5favX1cscd1J4eif2YcFNpLHMe9e1W80XaN9D8DhpcI8d5SEC6kHGbiv/lKnTh+atd57s+iSqVhE/B6IFAgt3kKbue86OQ==
     procedure_id : 1170889736540579775
