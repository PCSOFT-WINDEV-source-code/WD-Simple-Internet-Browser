        code : |1-
         procedure public URL(Value)
         
         // Load a system URL
         if Value [= Prefix_System_URL then
         	
         	// Processes the URL
         	ProcessSystemURL( Value )		
         else
         	
         	// Force the browser display
         	IWC_SYSTEM_TAB.ChangeSourceWindow("")
         	IWC_SYSTEM_TAB.Plane	= 1
         	HTM_BROWSER.Plane		= 0
         	
         	// The URL is already loaded
         	if Value = __URL _and_  Value <> "" then
         		
         		// Update required
         		HTM_BROWSER.Navigate(htmlRefresh)	
         	else
         		
         		// URL update
         		__URL	= Value
         		
         		// Synchronizes the edit control
         		EDT_BRW	= __URL
         		EDT_BRW.ExecuteProcess(trtExit)
         		
         		// Displays the page in the browser
         		HTM_BROWSER = __URL
         		SetFocus(HTM_BROWSER)
         	end
         end
        type : 2031616
