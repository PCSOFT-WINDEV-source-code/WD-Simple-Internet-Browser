           procedure_id : 1170889736541431743
           type_code : 14
           code : |1-
            // Summary: <specify the action of the procedure>
            // Syntax:
            // GetIcon ()
            //
            // Parameters:
            //	None
            // Return value:
            // 	None
            //
            procedure GetIcon()
            
            // Executes JS code
            HTM_BROWSER.ExécuteJS([
            	var e = document.querySelector('link[rel$="shortcut icon"]');
            	if ( e != null )
            		return e.href;	
            	return '';
            ], __OnExecuteJS )
            
            INTERNAL PROCEDURE __OnExecuteJS( bRes is boolean, sReturnValue is string )
            	
            	sFavicon is string = ""
            	
            	// If the execution was successful
            	if bRes then
            		
            		// If there is no value
            		if sReturnValue <> "" then
            			
            			// Stores the value as icon path
            			sFavicon = sReturnValue	
            		end
            	end
            	
            	if sFavicon = "" then
            		
            		// Builds a URL to access 'favicon.ico'
            		sFavicon = URLExtractPath(IW_BROWSER.URL, urlProtocol + urlDomain + urlPort + urlUser + urlPassword) + [ "/" ] + "favicon.ico"									
            	end
            	
            	DownloadFavicon:
            	
            	// Prepares the icon download
            	stFavicon is httpRequest
            	stFavicon.URL			= sFavicon
            	stFavicon.IgnoreError	= httpIgnoreRedirectToHTTPS + httpIgnoreRedirectToHTTP + httpIgnoreRedirection + httpIgnoreExpiredCertificate + httpIgnoreInvalidCertificate + httpIgnoreInvalidCertificateName
            	
            	// Downloads the icon
            	stResponse is httpResponse = stFavicon.Send()
            	
            	// Got a redirect?
            	if stResponse.StatusCode in ( 301, 302 ) then
            		
            		// Gets the new URL
            		sFavicon = stResponse.Header[ "Location" ]
            		
            		// Restarts the download
            		goto DownloadFavicon
            	end						
            	
            	// Good response
            	if stResponse.StatusCode = 200 then
            		
            		// Loads the icon
            		__icon = stResponse.Content
            		
            		// Changes the tab icon
            		ChangeTabIcon( __icon )				
            	end
            end
           type : 458752
