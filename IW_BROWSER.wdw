           code : |1-
            procedure RightClickLink( local sUrl is string )
            
            // Redefines the actions of the menu options
            
            
            // - Open link in a new tab
            MENU_LINK.OPT_LINK_NEW_TAB..Process[ trtMenuOption ] = () => {
            
            // Open the URL in a new browser
            OpenBrowser( sUrl, "" )
            }
            
            // - Open link in a new window
            MENU_LINK.OPT_LINK_NEW_WINDOW..Process[ trtMenuOption ] = () => {
            
            // Open the URL in a new browser
            OpenBrowser( sUrl, "", True, True )
            
            }
            
            // - Save link as
            MENU_LINK.OPT_LINK_SAVE_AS..Process[ trtMenuOption ] = () => {
            
            sFileName is string = URLExtractPath(sUrl, urlResourceName + urlResourceExtension)
            if sFileName = "" then
            	sFileName = "download.html"
            end
            
            // Opens the file picker
            sFile is string = fSelect("", sFileName, "Save as", "All files (*.*)" + TAB + "*.*", "*.*", fselCreate + fselExist)
            if sFile = "" then return
            
            // Prepares the download
            stDownload is httpRequest
            stDownload.URL			= sUrl
            stDownload.Destination	= sFile
            
            // Downloads the file
            stDownload.Send()
            
            // Inform
            ToastDisplay("Download completed", toastShort, vaMiddle, haCenter)
            }
            
            // - Copy link
            MENU_LINK.OPT_LINK_COPY..Process[ trtMenuOption ] = () => {
            
            // Copies the URL to the clipboard
            ToClipboard(sUrl) 
            
            // Inform
            ToastDisplay("Link copied", toastShort, vaMiddle, haCenter)
            }
            
            OpenPopupMenu(MENU_LINK)
