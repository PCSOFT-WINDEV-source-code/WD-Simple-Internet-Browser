      // Summary: Builds the history menu
      // Syntax:
      // BuildHistoryMenu ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      procedure BuildHistoryMenu()
      
      T			is int
      
      // Retrieves the history but removes duplicate URLs
      arrHistory	is array of HistoryEntry <= History
      arrHistory.DeleteDuplicate(asMember, "URL")
      
      MENU_SETTINGS.OPT_HISTORY.OPT_Separator5.Visible = ( arrHistory.Empty = False )
      
      // Up to 10 history entries are allowed
      for i = 1 _to_ 10
      	
      	T ++
      	oOption is Control <- { "OPT_HISTORY_ENTRY_" + i, indControl }
      	
      	if T > arrHistory.Count then
      		oOption.Visible = False
      		continue
      	end
      	
      	// References the history entry
      	stEntry is HistoryEntry = arrHistory[ T ]
      	
      	// If the URL of the entry is the same as the one displayed in the browser
      	while stEntry.URL = IW_BROWSER.URL
      		
      		// search for the following entry
      		T ++
      		
      		// If there are no more history entries available
      		if T > arrHistory.Count then
      			
      			// Hides the option and continues the iteration
      			oOption.Visible = False
      			continue(2)
      		end 
      		
      		stEntry = arrHistory[ T ]			
      	end
      	
      	// Changes the option
      	oOption.Visible						= True
      	oOption.grise						= False
      	oOption.Libellé						= stEntry.Title
      	oOption.Image						= ( DataURIToImage( stEntry.icon, 16, 16 ) )
      	
      	// Modifies the click process on the menu option: generate a procedure call
      	oOption.Traitement[ trtMenuOption ]	= __OpenUrl( stEntry.URL )			
      end
      
      INTERNAL PROCEDURE __OpenUrl( local sUrl is string )
      	
      	// Creates a procedure that will open the tab with the URL
      	pOpenBrwsr is procedure = __OpenUrlMenuOption
      	
      	INTERNAL PROCEDURE __OpenUrlMenuOption()
      		// Opens a tab with the URL
      		OpenBrowser( sUrl, "", False )
      	end
      	
      	return pOpenBrwsr
      end
