      procedure ChangeTabIcon( local oFavicon is Image )
      
      // Resizes the image
      oFavicon.Resize(16, 16, drHighQuality + drHomotheticCentered)
      
      // Creates a slightly larger image to leave a margin between the icon and the title
      oImageTab is Image
      oImageTab.Width		= oFavicon.Width + 8
      oImageTab.Height	= oFavicon.Height
      
      // Copies the favicon to the tab image
      oFavicon.Copy(oImageTab, 0, 0, oFavicon.Width, oFavicon.Height, 0, 0, oFavicon.Width, oFavicon.Height)
      
      // Assigns the image to the tab
      __Pane.Image = oImageTab
