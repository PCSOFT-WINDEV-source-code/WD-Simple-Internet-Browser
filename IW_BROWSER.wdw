            __Source = SourceInput
            
            sUrl is string = EDT_BRW
            
            // Removes spaces
            sUrl = sUrl.NoSpace()
            
            // If the values entered correspond to a URL
            if sUrl.MatchRegularExpression(REGEXPURL) then
            	
            	// Assign the URL
            	IW_BROWSER.URL = sUrl
            	
            	// It's a file
            else if sUrl [~ "file:" _and_ fFileExist(fSeparator(sUrl[ 9 to ], fSeparatorPlatform))
            	
            	// Assign the file
            	IW_BROWSER.URL = sUrl
            	
            	// It's a system URL
            else if sUrl [= Prefix_System_URL then 
            	
            	// Processes the URL
            	ProcessSystemURL( sUrl )
            	
            else
            	
            	// Triggers a search
            	TriggerSearch( sUrl )
            end
