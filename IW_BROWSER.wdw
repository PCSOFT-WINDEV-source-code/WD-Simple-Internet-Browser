           name : GetTitle
           internal_properties : CAAAAAgAAAAeAd8I/SDyqU0gk5Cn/iy7cWfxVByjmUAE78SMJmRSUZZCXryTRZvTV8D9+k9wQloJtiiAuUL0sRhD/10GQhxV0H7Up8uBlaOD5ZwOM4TFrmQOdGVLaf0Q+nvqfnF/yLMHydGhOWwWhNyCo82KyN2yVxTdOthEIuNipFPsxL0Yj48qhxhV0wTFLk8XoHPlc8ta
           procedure_id : 1170889736541497279
           type_code : 14
           code : |1-
            // Summary: Gets the title of the page currently displayed
            // Syntax:
            //GetTitle ()
            //
            // Parameters:
            //	None
            // Return value:
            // 	None
            //
            procedure GetTitle() 
            
            // Executes JS code
            HTM_BROWSER.ExecuteJS("document.location.href + '\t' + document.title;", __OnExecuteJS)
            
            INTERNAL PROCEDURE __OnExecuteJS( bRes is boolean, sReturnValue is string )
            	
            	// If the execution was successful
            	if bRes then
            		
            		// Retrieves the url
            		sUrl is string = sReturnValue.ExtractString(1)
            		
            		// Displays the URL in the edit control
            		EDT_BRW = sUrl
            		EDT_BRW.ExecuteProcess(trtExit)	
            		EDT_BRW.State			= Active
            		
            		// Stores the URL
            		__URL					= sUrl
            		
            		IW_BROWSER.PageTitle	= sReturnValue.ExtractString(2)
            		
            		// Gets the site icon
            		GetIcon()
            		
            		// Processes history and favorites
            		ProcessHistoryFavorite()
            	end
            end
