      // Summary: Adds the tab to favorites
      // Syntax:
      // FavoriteAddTab ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      procedure FavoriteAddTab()
      
      pstFavorite	is FavoriteEntry dynamic
      bAdd		is boolean
      sTitle		is string	= IW_BROWSER.PageTitle
      sUrl		is string	= IW_BROWSER.URL
      
      // The site has already been added to favorites
      if BTN_FAVORITES_ADD.Note  <> favoriteUnknown then
      	
      	// Finds the favorite
      	let N = Favorites.Seek(asLinear, "URL", sUrl)
      	
      	// If the favorite was found
      	if N > 0 then
      		
      		// Keep a reference on the entry
      		pstFavorite = Favorites[ N ]
      	end
      	
      end
      
      // If the favorite is still unknown
      if pstFavorite = Null _or_ BTN_FAVORITES_ADD.Note = favoriteUnknown then
      	
      	// Creates an entry
      	stFavorite is FavoriteEntry
      	stFavorite.Date		= SysDateTime
      	stFavorite.Name		= sTitle
      	stFavorite.URL		= sUrl
      	stFavorite.icon		= ImageToDataURI( __icon )
      	
      	// Adds the entry
      	let N = Favorites.Add(stFavorite)	
      	
      	// Keep a reference on the entry
      	pstFavorite	= Favorites[ N ]
      	
      	// Notify the popup that the entry was created
      	bAdd		= True
      	
      end
      
      // Opens the add/edit favorites window
      WIN_FAVORITE.OpenPopupPosition(poAccordingToControl + poRight, BTN_FAVORITES_ADD, pstFavorite, bAdd)
      
      // - If the favorite has been deleted	
      // - If the URL of the favorite has been modified
      UrlTestFavorite( sUrl )
      
      // Saves the favorites
      SaveFavorites()
