           code : |1-
            procedure ClickLink( sUrl is string, bExternalTarget is boolean, sTargetName is string  )
            
            // Clicking on the link sends to another browser
            if bExternalTarget then
            	
            	// Process according to the name of the target
            	switch sTargetName.Lower()
            	
            		// target="_blank"
            		case targetBlank
            			
            			// Open the URL in a new browser
            			OpenBrowserDelay( sUrl, "" )
            			return true
            			
            		other case
            			
            			// Open the URL in a new browser with alias
            			OpenBrowserDelay( sUrl, sTargetName )
            			RETURN True
            	end
            	
            end
            
            
            // Displays the URL in the edit control
            EDT_BRW = sUrl
            EDT_BRW.ExecuteProcess(trtExit)	
            EDT_BRW.State	= Active
            
            // Stores the URL
            __URL			= sUrl
            
            return True
